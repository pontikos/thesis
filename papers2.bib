@article{Trynka:2011jl,
author = {Trynka, Gosia and Hunt, Karen A and Bockett, Nicholas A and Romanos, Jihane and Mistry, Vanisha and Szperl, Agata and Bakker, Sjoerd F and Bardella, Maria Teresa and Bhaw-Rosun, Leena and Castillejo, Gemma and de la Concha, Emilio G and de Almeida, Rodrigo Coutinho and Dias, Kerith-Rae M and van Diemen, Cleo C and Dubois, Patrick C A and Duerr, Richard H and Edkins, Sarah and Franke, Lude and Fransen, Karin and Gutierrez, Javier and Heap, Graham A R and Hrdlickova, Barbara and Hunt, Sarah and Izurieta, Leticia Plaza and Izzo, Valentina and Joosten, Leo A B and Langford, Cordelia and Mazzilli, Maria Cristina and Mein, Charles A and Midah, Vandana and Mitrovic, Mitja and Mora, Barbara and Morelli, Marinita and Nutland, Sarah and N{\'u}{\~n}ez, Concepci{\'o}n and Onengut-Gumuscu, Suna and Pearce, Kerra and Platteel, Mathieu and Polanco, Isabel and Potter, Simon and Ribes-Koninckx, Carmen and Rica{\~n}o-Ponce, Isis and Rich, Stephen S and Rybak, Anna and Santiago, Jos{\'e} Luis and Senapati, Sabyasachi and Sood, Ajit and Szajewska, Hania and Troncone, Riccardo and Varad{\'e}, Jezabel and Wallace, Chris and Wolters, Victorien M and Zhernakova, Alexandra and {Spanish Consortium on the Genetics of Coeliac Disease (CEGEC)} and {PreventCD Study Group} and {Wellcome Trust Case Control Consortium (WTCCC)} and Thelma, B K and Cukrowska, Bozena and Urcelay, Elena and Bilbao, Jose Ramon and Mearin, M Luisa and Barisani, Donatella and Barrett, Jeffrey C and Plagnol, Vincent and Deloukas, Panos and Wijmenga, Cisca and van Heel, David A},
title = {{Dense genotyping identifies and localizes multiple common and rare variant association signals in celiac disease.}},
journal = {Nature Genetics},
year = {2011},
volume = {43},
number = {12},
pages = {1193--1201},
month = dec,
affiliation = {Genetics Department, University Medical Center and University of Groningen, The Netherlands.},
keywords = {immunochip},
doi = {10.1038/ng.998},
pmid = {22057235},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-01-16T13:40:41GMT},
date-modified = {2013-08-06T00:52:39GMT},
abstract = {Using variants from the 1000 Genomes Project pilot European CEU dataset and data from additional resequencing studies, we densely genotyped 183 non-HLA risk loci previously associated with immune-mediated diseases in 12,041 individuals with celiac disease (cases) and 12,228 controls. We identified 13 new celiac disease risk loci reaching genome-wide significance, bringing the number of known loci (including the HLA locus) to 40. We found multiple independent association signals at over one-third of these loci, a finding that is attributable to a combination of common, low-frequency and rare genetic variants. Compared to previously available data such as those from HapMap3, our dense genotyping in a large sample collection provided a higher resolution of the pattern of linkage disequilibrium and suggested localization of many signals to finer scale regions. In particular, 29 of the 54 fine-mapped signals seemed to be localized to single genes and, in some instances, to gene regulatory elements. Altogether, we define the complex genetic architecture of the risk regions of and refine the risk signals for celiac disease, providing the next step toward uncovering the causal mechanisms of the disease.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22057235&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2011/Trynka/Nat.%20Genet.%202011%20Trynka.pdf},
file = {{Nat. Genet. 2011 Trynka.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2011/Trynka/Nat. Genet. 2011 Trynka.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/ng.998}}
}

@article{Nikula:2005bh,
author = {Nikula, T and West, A and Katajamaa, M and Lonnberg, T and Sara, R and Aittokallio, T and Nevalainen, OS and Lahesmaa, R},
title = {{A human ImmunoChip cDNA microarray provides a comprehensive tool to study immune responses}},
journal = {Journal of immunological methods},
year = {2005},
volume = {303},
number = {1-2},
pages = {122--134},
affiliation = {Turku Ctr Biotechnol, FIN-20521 Turku, Finland},
doi = {10.1016/j.jim.2005.06.004},
pmid = {16054160},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-01-16T13:48:20GMT},
date-modified = {2012-01-23T04:06:58GMT},
abstract = {DNA microarray technology has developed rapidly in recent years and has become an essential tool, providing novel approaches to biomedical research. In this paper, we describe a self-designed ImmunoChip cDNA array for immunological research. With a comprehensive selection of genes of interest, we can focus on key signalling pathways and molecular mechanisms at relatively low cost compared to commercial platforms which are usually targeted at global screening of gene expression. To validate the efficiency of the ImmunoChip, we studied T helper cell polarization to functionally distinct subsets (Th1 and Th2). We also developed a tool for quality control of cDNA microarrays that assesses the technical quality of an ImmunoChip. The information produced with the quality control tool is shown to be valuable for extracting correct information from cDNA microarrays. Gene expression measurements with ImmunoChip are in agreement with the results obtained using oligonucleotide microarrays and with published quantitative RT-PCR data. The ImmunoChip provides reliable measurements and gives new insights into various aspects of human immune responses. (c) 2005 Elsevier B.V. All rights reserved.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16054160&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2005/Nikula/J.%20Immunol.%20Methods%202005%20Nikula.pdf},
file = {{J. Immunol. Methods 2005 Nikula.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2005/Nikula/J. Immunol. Methods 2005 Nikula.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.jim.2005.06.004}}
}

