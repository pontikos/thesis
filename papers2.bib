@article{Cordovado2009747,
author = {Cordovado, Suzanne Kehoe and Hancock, Laura N and Hendrix, Miyono and Greene, Christopher N and Mueller, Patricia W},
title = {{Novel human leukocyte antigen class I and class II alleles identified by sequence-based typing in the Genetics of Kidneys in Diabetes (GoKinD) study population}},
