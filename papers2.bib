author = {Boedigheimer, Michael J and Ferbas, John},
title = {{Mixture modeling approach to flow cytometry data.}},
journal = {Cytometry Part A},
year = {2008},
volume = {73},
number = {5},
pages = {421--429},
month = may,
annote = {very basic MM of Gaussian in Matlab, no software provided.
number of dimensions 4
number of components chosen to be 6},
affiliation = {Computational Biology, Amgen, Thousand Oaks, California, USA. mboedigh@amgen.com},
doi = {10.1002/cyto.a.20553},
