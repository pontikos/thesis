@article{Sampson:2012hk,
author = {Sampson, Joshua N and Jacobs, Kevin and Wang, Zhaoming and Yeager, Meredith and Chanock, Stephen and Chatterjee, Nilanjan},
title = {{A two-platform design for next generation genome-wide association studies.}},
journal = {Genetic epidemiology},
year = {2012},
volume = {36},
number = {4},
pages = {400--408},
month = may,
affiliation = {Biostatistics Branch, DCEG, National Cancer Institute, Rockville, Maryland 20852, USA. joshua.sampson@nih.gov},
doi = {10.1002/gepi.21634},
pmid = {22508365},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-08-21T20:18:44GMT},
date-modified = {2013-09-30T11:16:58GMT},
abstract = {Genome-wide association studies (GWAS) have been successful in their search for common genetic variants associated with complex traits and diseases. With new advances in array technologies together with available genetic reference sets, the next generation of GWAS will extend the search for associations with uncommon SNPs (1% ≤ MAF ≤ 10%). Two possible approaches are genotyping all participants, a prohibitively expensive option for large GWAS, or using a combination of genotyping and imputation. Here, we consider a two platform method that genotypes all participants on a standard genotyping array, designed to identify common variants, and then supplements that data by genotyping only a small proportion of the participants on a platform that has higher coverage for uncommon SNPs. This subset of the study population is then included as part of the imputation reference set. To demonstrate the use of this two-platform design, we evaluate its potential efficiency using a newly available dataset containing 756 individuals genotyped on both the Illumina Human OmniExpress and Omni2.5 Quad. Although genotyping all individuals on the denser array would be ideal, we find that genotyping only 100 individuals on this array, in combination with imputation, leads to only a modest loss of power for detecting associations. However, the loss of power due to imputation can be more substantial if the relative risks for rare variants are significantly larger than those previously observed for common variants.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22508365&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2012/Sampson/Genet.%20Epidemiol.%202012%20Sampson.pdf},
file = {{Genet. Epidemiol. 2012 Sampson.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2012/Sampson/Genet. Epidemiol. 2012 Sampson.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/gepi.21634}}
}

