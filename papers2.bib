@incollection{You200813,
author = {You, Sylvaine and Candon, Sophie and Kuhn, Chantal and Bach, Jean-Fran{\c c}ois and Chatenoud, Lucienne},
title = {{Chapter 2 CD3 Antibodies as Unique Tools to Restore Self-Tolerance in Established Autoimmunity: Their Mode of Action and Clinical Application in Type 1 Diabetes}},
year = {2008},
