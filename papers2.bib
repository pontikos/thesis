file = {{Genome Res. 2006 Ravasi.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2006/Ravasi/Genome Res. 2006 Ravasi.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1101/gr.4200206}}
}

