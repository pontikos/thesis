@article{Crosby2011,
author = {Crosby, Kevin C and Pietraszewska-Bogiel, Anna and Gadella, Theodorus W J and Winkel, Brenda S J},
title = {{F\"orster resonance energy transfer demonstrates a flavonoid metabolon in living plant cells that displays competitive interactions between enzymes.}},
journal = {FEBS letters},
year = {2011},
volume = {585},
number = {14},
pages = {2193--2198},
publisher = {Federation of European Biochemical Societies},
keywords = {Fluorescence,Microscopy, Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: metabolism,Arabidopsis: chemistry,Arabidopsis: cytology,Arabidopsis: metabolism,Flavonoids,Flavonoids: chemistry,Flavonoids: metabolism,Fluorescence Resonance Energy Transfer,Fluorescence Resonance Energy Transfer: methods,Microscopy, Fluorescence: methods},
