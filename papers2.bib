@article{Fernandes20041047,
author = {Fernandes, Ana Paula M and Foss, Milton C and Ramos, Silvia B V and Donadi, Eduardo A},
title = {{Overexpression of HLA class I molecules on T cells among type 1 diabetes Brazilian patients}},
journal = {Molecular Immunology},
year = {2004},
