@article{Persengiev2010598,
author = {Persengiev, Stephan and {Bobby P.C. Koeleman} and Downes, Kate and Valdigem, Gustavo and van der Slik, Arno R and Eerligh, Peter and Monsuur, Alienke and Bruining, G Jan and Wijmenga, Cisca and Todd, John A and Roep, Bart O and Alizadeh, Behrooz Z},
title = {{Association analysis of myosin IXB and type 1 diabetes}},
journal = {Human Immunology},
