@article{Pihoker:2005tc,
author = {Pihoker, Catherine and Gilliam, Lisa K and Hampe, Christiane S and Lernmark, Ake},
title = {{Autoantibodies in diabetes.}},
journal = {Diabetes},
year = {2005},
volume = {54 Suppl 2},
pages = {S52--61},
month = dec,
affiliation = {Department of Pediatrics, University of Washington, Seattle, Washington 98195, USA. catherine.pihoker@seattlechildrens.org},
pmid = {16306341},
language = {English},
rating = {0},
date-added = {2012-01-04T21:24:37GMT},
date-modified = {2012-06-21T11:05:26GMT},
abstract = {Islet cell autoantibodies are strongly associated with the development of type 1 diabetes. The appearance of autoantibodies to one or several of the autoantigens-GAD65, IA-2, or insulin-signals an autoimmune pathogenesis of beta-cell killing. A beta-cell attack may be best reflected by the emergence of autoantibodies dependent on the genotype risk factors, isotype, and subtype of the autoantibodies as well as their epitope specificity. It is speculated that progression to beta-cell loss and clinical onset of type 1 diabetes is reflected in a developing pattern of epitope-specific autoantibodies. Although the appearance of autoantibodies does not follow a distinct pattern, the presence of multiple autoantibodies has the highest positive predictive value for type 1 diabetes. In the absence of reliable T-cell tests, dissection of autoantibody responses in subjects of genetic risk should prove useful in identifying triggers of islet autoimmunity by examining seroconversion and maturation of the autoantibody response that may mark time to onset of type 1 diabetes. The complexity of the disease process is exemplified by multiple clinical phenotypes, including autoimmune diabetes masquerading as type 2 diabetes in youth and adults. Autoantibodies may also provide prognostic information in clinically heterogeneous patient populations when examined longitudinally.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16306341&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2005/Pihoker/Diabetes%202005%20Pihoker.pdf},
file = {{Diabetes 2005 Pihoker.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2005/Pihoker/Diabetes 2005 Pihoker.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/243A9EC0-42B6-4813-A867-83AF4C335A60}}
}

