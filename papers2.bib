@article{Aly200540,
author = {Aly, Theresa A and Ide, Akane and Humphrey, Kurt and Barker, Jennifer M and Steck, Andrea and Erlich, Henry A and Yu, Liping and Miao, Dongmei and Redondo, Maria J and McFann, Kim and Roberts, Christine M and Babu, Sunanda R and Norris, Jill M and Eisenbarth, George S and Rewers, Marian J},
