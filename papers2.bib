@article{Cooper:2012il,
author = {Cooper, Jason D and Simmonds, Matthew J and Walker, Neil M and Burren, Oliver and Brand, Oliver J and Guo, Hui and Wallace, Chris and Stevens, Helen and Coleman, Gillian and {Wellcome Trust Case Control Consortium} and Franklyn, Jayne A and Todd, John A and Gough, Stephen C L},
title = {{Seven newly identified loci for autoimmune thyroid disease.}},
journal = {Human molecular genetics},
year = {2012},
volume = {21},
number = {23},
pages = {5202--5208},
month = dec,
affiliation = {Department of Medical Genetics, Cambridge Institute for Medical Research, Juvenile Diabetes Research Foundation/ Wellcome Trust Diabetes and Inflammation Laboratory, NIHR Cambridge Biomedical Research Centre, University of Cambridge, Addenbrooke's Hospital, Cambridge, UK. jason.cooper@cimr.cam.ac.uk},
doi = {10.1093/hmg/dds357},
pmid = {22922229},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-07-30T14:05:25GMT},
date-modified = {2013-08-10T17:53:30GMT},
abstract = {Autoimmune thyroid disease (AITD), including Graves' disease (GD) and Hashimoto's thyroiditis (HT), is one of the most common of the immune-mediated diseases. To further investigate the genetic determinants of AITD, we conducted an association study using a custom-made single-nucleotide polymorphism (SNP) array, the ImmunoChip. The SNP array contains all known and genotype-able SNPs across 186 distinct susceptibility loci associated with one or more immune-mediated diseases. After stringent quality control, we analysed 103 875 common SNPs (minor allele frequency >0.05) in 2285 GD and 462 HT patients and 9364 controls. We found evidence for seven new AITD risk loci (P < 1.12 × 10(-6); a permutation test derived significance threshold), five at locations previously associated and two at locations awaiting confirmation, with other immune-mediated diseases.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22922229&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2012/Cooper/Hum.%20Mol.%20Genet.%202012%20Cooper.pdf},
