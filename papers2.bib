volume = {24},
number = {1},
pages = {35--42},
doi = {10.1016/j.pt.2007.10.006},
rating = {0},
date-added = {2012-11-29T11:46:05GMT},
date-modified = {2012-11-29T11:46:05GMT},
abstract = {As more facts emerge regarding the ways in which parasite-derived molecules modulate the host immune response, it is possible to envisage how a lack of infection by agents that once infected humans commonly might contribute to the rise in autoimmune disease. Through effects on cells of both the innate and adaptive arms of the immune response, parasites can orchestrate a range of outcomes that are beneficial not only to parasites, in terms of facilitating their life cycles, but also to their host, in limiting pathology.},
url = {http://www.sciencedirect.com/science/article/pii/S1471492207002899},
uri = {\url{papers2://publication/doi/10.1016/j.pt.2007.10.006}}
