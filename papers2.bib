@article{Vilches:2002ii,
author = {Vilches, Carlos and Parham, Peter},
title = {{KIR: diverse, rapidly evolving receptors of innate and adaptive immunity.}},
journal = {Annual review of immunology},
year = {2002},
volume = {20},
pages = {217--251},
affiliation = {Servicio de Inmunolog{\'\i}a, Hospital Universitario Cl{\'\i}nica Puerta de Hierro, San Mart{\'\i}n de Porres 4, 28035 Madrid, Spain. incvilches@inicia.es},
doi = {10.1146/annurev.immunol.20.092501.134942},
pmid = {11861603},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-11-26T15:01:34GMT},
date-modified = {2012-12-10T14:21:34GMT},
abstract = {KIR genes have evolved in primates to generate a diverse family of receptors with unique structures that enable them to recognize MHC-class I molecules with locus and allele-specificity. Their combinatorial expression creates a repertoire of NK cells that surveys the expression of almost every MHC molecule independently, thus antagonizing the spread of pathogens and tumors that subvert innate and adaptive defense by selectively downregulating certain MHC class I molecules. The genes encoding KIR that recognize classical MHC molecules have diversified rapidly in human and primates; this contrasts with conservation of immunoglobulin- and lectin-like receptors for nonclassical MHC molecules. As a result of the variable KIR-gene content in the genome and the polymorphism of the HLA system, dissimilar numbers and qualities of KIR:HLA pairs function in different humans. This diversity likely contributes variability to the function of NK cells and T-lymphocytes by modulating innate and adaptive immune responses to specific challenges.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=11861603&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2002/Vilches/Annu.%20Rev.%20Immunol.%202002%20Vilches.pdf},
file = {{Annu. Rev. Immunol. 2002 Vilches.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2002/Vilches/Annu. Rev. Immunol. 2002 Vilches.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1146/annurev.immunol.20.092501.134942}}
}

@article{Sham:2000fg,
author = {Sham, P C and Cherny, S S and Purcell, S and Hewitt, J K},
title = {{Power of linkage versus association analysis of quantitative traits, by use of variance-components models, for sibship data.}},
journal = {American Journal of Human Genetics},
year = {2000},
volume = {66},
number = {5},
pages = {1616--1630},
month = may,
affiliation = {Social, Genetic and Developmental Psychiatry Research Centre, Institute of Psychiatry, London SE5 8AF, United Kingdom. spjupcs@iop.kcl.ac.uk.},
doi = {10.1086/302891},
pmid = {10762547},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-08-27T18:05:14GMT},
date-modified = {2013-09-30T11:17:01GMT},
abstract = {Optimal design of quantitative-trait loci (QTL) mapping studies requires a precise understanding of the power of QTL linkage versus QTL association analysis, under a range of different conditions. In this article, we investigate the power of QTL linkage and association analyses for simple random sibship samples, under the variance-components model proposed by Fulker et al. After a brief description of an extension of this variance-components model, we show that the powers of both linkage and association analyses are crucially dependent on the proportion of phenotypic variance attributable to the QTL. The main difference between the two tests is that, whereas the power of association is directly related to the QTL heritability, the power of linkage is related more closely to the square of the QTL heritability. We also describe both how the power of linkage is attenuated by incomplete linkage and incomplete marker information and how the power of association is attenuated by incomplete linkage disequilibrium.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10762547&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2000/Sham/Am%20J%20Hum%20Genet%202000%20Sham.pdf},
file = {{Am J Hum Genet 2000 Sham.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2000/Sham/Am J Hum Genet 2000 Sham.pdf:application/pdf;Am J Hum Genet 2000 Sham.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2000/Sham/Am J Hum Genet 2000 Sham.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/302891}}
}

@article{Carr:2007tj,
author = {Carr, William H and Rosen, David B and Arase, Hisashi and Nixon, Douglas F and Michaelsson, Jakob and Lanier, Lewis L},
title = {{Cutting Edge: KIR3DS1, a gene implicated in resistance to progression to AIDS, encodes a DAP12-associated receptor expressed on NK cells that triggers NK cell activation.}},
