@incollection{MaciejewskaRodrigues2011535,
author = {Maciejewska-Rodrigues, Hanna and J{\"u}ngel, Astrid and Gay, Steffen},
title = {{Chapter 33 - The Role of Epigenetics in Immune Disorders}},
booktitle = {Handbook of Epigenetics},
year = {2011},
editor = {Tollefsbol, Trygve},
pages = {535--551},
publisher = {Academic Press},
address = {San Diego},
doi = {10.1016/B978-0-12-375709-8.00033-2},
isbn = {978-0-12-375709-8},
rating = {0},
date-added = {2012-11-29T11:45:42GMT},
date-modified = {2012-11-29T11:46:43GMT},
abstract = {Publisher Summary This chapter discusses the role of epigenetics in immune disorders. The immune system enables the organism to resist infections and is composed of the innate or non-specific system and the adaptive or specific system. Aberrations in any part of it can lead to misdirected reactions resulting in autoimmunity where the immune system attack turns against the host tissues. Research has focused on elaborating the epigenetic regulation of the forkhead box P3---a transcriptional repressor and the master regulator of development and function of Treg cells. It has been shown that CpG motifs in the locus of Foxp3 are demethylated in Tregs in contrast to naive T cells. Furthermore, also histone modifications such as increased histone acetylation in the promoter of Foxp3 have been described in natural Tregs. Stimulation of Treg cells by ligation of TCR leads to the nuclear translocation of Foxp3 and its binding to promoters of proinflammatory IL-2 and INF-$\gamma$ inducing their deacetylation and silencing, as well as to promoters of GITR, CD25, and CTLA-4 increasing their acetylation and expression which is characteristic for the functional Tregs. It was shown that Foxp3 is optimally functional when in a complex with histone acetyltransferase Tip60 and histone deacetylase HDAC7. Summary Autoimmune disorders are characterized by self directed immune responses. For most of the autoimmune diseases the pathogenesis remains unclear. However, the contribution of epigenetic modifications is recognized in systemic lupus erythematosus (SLE) and has recently emerged to be modified in rheumatoid arthritis (RA) as well as few other autoimmune diseases such as multiple sclerosis (MS), type 1 diabetes, systemic sclerosis (SSc) and the immunodeficiency, centromeric instability and facial abnormalities (ICF syndrome). Thus, in this chapter we focus on the pattern of epigenetic modifications such as DNA methylation, histone acetylation, and gene expression regulating microRNA described in these immune disorders.},
url = {http://www.sciencedirect.com/science/article/pii/B9780123757098000332},
uri = {\url{papers2://publication/doi/10.1016/B978-0-12-375709-8.00033-2}}
}

