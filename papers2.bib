@article{Brinkman:2007ib,
author = {Brinkman, Ryan Remy and Gasparetto, Maura and Lee, Shang-Jung Jessica and Ribickas, Albert J and Perkins, Janelle and Janssen, William and Smiley, Renee and Smith, Clay},
title = {{High-content flow cytometry and temporal data analysis for defining a cellular signature of graft-versus-host disease.}},
journal = {Biology of blood and marrow transplantation : journal of the American Society for Blood and Marrow Transplantation},
year = {2007},
