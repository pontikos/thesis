@article{Long2009921,
author = {Long, Meixiao and Park, Sung-Gyoo and Strickland, Ian and Hayden, Matthew S and Ghosh, Sankar},
title = {{Nuclear Factor-$\kappa$B Modulates Regulatory T Cell Development by Directly Regulating Expression of Foxp3 Transcription Factor}},
journal = {Immunity},
