@article{Khalangot2009611,
author = {Khalangot, M and Kravchenko, V and Tronko, M and Gur'ianov, V},
title = {{Correlation between the prevalence of type 1 diabetes with the daily insulin dose and the autoimmune process against glutamic acid decarboxylase in adults}},
journal = {European Journal of Internal Medicine},
year = {2009},
