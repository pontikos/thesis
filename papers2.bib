@article{Shevach2012626,
author = {Shevach, Ethan M},
title = {{Application of IL-2 therapy to target T regulatory cell function}},
journal = {Trends in Immunology},
year = {2012},
volume = {33},
number = {12},
pages = {626--632},
keywords = {immune complexes},
doi = {10.1016/j.it.2012.07.007},
read = {Yes},
rating = {0},
date-added = {2012-11-29T11:46:03GMT},
date-modified = {2013-10-28T11:13:32GMT},
abstract = {Interleukin-2 (IL-2) was originally discovered as a growth factor for activated T cells in vitro. IL-2 promotes CD8+ T cell growth and differentiation in vivo, but has little effect on CD4+ T cell function. Regulatory T cells (Treg cells) express all three chains (CD25, CD122, and CD132) of the IL-2 receptor complex and are dependent on IL-2 for survival and function. Exogenous IL-2 can augment Treg cell numbers in vivo and may have therapeutic value in the treatment of autoimmune and inflammatory diseases. Complexes of IL-2 with different IL-2 antibodies can target delivery to cells expressing all three receptor chains (Treg cells and activated T effector cells) or to cells expressing just CD122 and CD132 (NK cells and memory phenotype CD8+ T cells).},
url = {http://www.sciencedirect.com/science/article/pii/S1471490612001299},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2012/Shevach/Trends%20in%20Immunology%202012%20Shevach.pdf},
