number = {6},
pages = {676--681},
annote = {<ce:title>Autoimmunity / Allergy and Hypersensitivity</ce:title> <ce:subtitle>Edited by Ciriaco Piccirillo and Roberta Pelanda / Bruce Bochner and Rafeul Alam</ce:subtitle>},
