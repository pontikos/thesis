title = {{Linking folding and binding.}},
journal = {Current Opinion in Structural Biology},
year = {2009},
volume = {19},
number = {1},
pages = {31--38},
keywords = {Animals,Binding Sites,Databases,Humans,Kinetics,Models,Molecular,Phosphorylation,Protein,Protein Conformation,Protein Folding,Proteins,Proteins: chemistry,Proteins: metabolism,Signal Transduction},
