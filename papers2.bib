pages = {4947--4957},
keywords = {Animals,Cell Communication,Cell Communication: genetics,Cell Communication: physiology,Evolution, Biological,Protein Interaction Mapping,Protein Interaction Mapping: methods,Protein Interaction Mapping: statistics \{\&} numerica,Signal Transduction,Signal Transduction: genetics,Signal Transduction: physiology, Molecular,Humans,Models},
