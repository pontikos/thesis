journal = {Methods in enzymology},
year = {2010},
volume = {470},
pages = {281--315},
keywords = {Bacteria,Bacteria: genetics,Bacteria: metabolism,Computational Biology,Genetic,Genetic: genetics,Polymerase Chain Reaction,Protein Binding,Protein Interaction Mapping,Protein Interaction Mapping: methods,Transformation,Two-Hybrid System Techniques,Yeasts,Yeasts: genetics,Yeasts: metabolism,interactome},
