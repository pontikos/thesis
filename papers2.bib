@article{Newell:2012dq,
author = {Newell, Evan W and Sigal, Natalia and Bendall, Sean C and Nolan, Garry P and Davis, Mark M},
title = {{Cytometry by Time-of-Flight Shows Combinatorial Cytokine Expression and Virus-Specific Cell Niches within a Continuum of CD8(+) T Cell Phenotypes.}},
journal = {Immunity},
year = {2012},
volume = {36},
number = {1},
pages = {142--152},
month = jan,
affiliation = {Department of Microbiology and Immunology, Stanford University, Stanford, CA 94305, USA.},
