@article{Alizadeh20089,
author = {Alizadeh, Behrooz Z and {Bobby P.C. Koeleman}},
title = {{Genetic polymorphisms in susceptibility to Type 1 Diabetes}},
journal = {Clinica Chimica Acta},
year = {2008},
volume = {387},
number = {1--2},
pages = {9--17},
keywords = {PTPN22},
doi = {10.1016/j.cca.2007.09.021},
rating = {0},
date-added = {2012-11-29T11:46:11GMT},
date-modified = {2012-11-29T11:46:11GMT},
abstract = {Type 1 Diabetes is a serious complex disease caused by several environmental and genetic factors. It is one of most common childhood diseases, requires life-long treatment, and is associated with increased mortality, mainly due to complications that occur later in life. More than three decades of genetic studies have identified several genetic disease variants and a longer list of putative associated genetic loci. These findings have greatly increased our understanding of the genetic background of T1D and have encouraged the development of genetic tools for mapping complex diseases. Here we review the wealth of data on T1D and discuss the major genetic polymorphisms involved in the disease. We place some putative genetic risk factors in perspective and look at those still to be detected.},
url = {http://www.sciencedirect.com/science/article/pii/S0009898107004846},
uri = {\url{papers2://publication/doi/10.1016/j.cca.2007.09.021}}
}

@article{Takahashi1995287,
author = {Takahashi, Kazuma and Satoh, Jo and Sagara, Mikio and Muto, Gen and Muto, Yoshiko and Fukuzawa, Masamitsu and Nishimura, Sachiko and Miyaguchi, Syuichi and Qiang, Xiao Ling and Kumagai, Katsuo and Toyota, Takayoshi},
title = {{Reduced Expression of c-Fos in Female NOD Mouse Thymocytes and Up-Regulation with Human Lymphotoxin}},
journal = {Cellular Immunology},
year = {1995},
volume = {164},
number = {2},
pages = {287--294},
doi = {10.1006/cimm.1995.1172},
rating = {0},
date-added = {2012-11-29T11:46:11GMT},
date-modified = {2012-11-29T11:46:11GMT},
abstract = {Previously we reported that the administration of human (h) lymphotoxin (h-LT) markedly protected NOD mice from insulin-dependent diabetes mellitus (IDDM) partly by affecting the generation phase of anti-islet effector cells, probably in the thymus. In this study, we investigated the effect of h-LT on the signal transduction of the mouse thymocytes by observing c-Fos expression in the thymocytes by using a flow cytometer. The intensity of c-Fos expression in whole thymocytes was significantly lower in the female NOD with a high incidence of diabetes than that in the male NOD mice with a low incidence of diabetes and than that in normal mice (P {\&}lt; 0.0001). The low c-Fos expression in the female NOD thymocytes was most prominent in CD3low thymocytes. c-Jun expression of the CD3low thymocytes was also lower in the female NOD mice. Administrations of h-LT, h-TNF, and h-IL-2, which has been reported to prevent IDDM in NOD mice by systemic administration, significantly up-regulated c-Fos expression in CD3low thymocytes. From these results, it is assumed that a relationship may exist between the high diabetes incidence and the defective c-Fos expression in female NOD mice and between the prevention of IDDM and the amelioration of the defective c-Fos expression with h-LT in female NOD mice.},
url = {http://www.sciencedirect.com/science/article/pii/S0008874985711720},
uri = {\url{papers2://publication/doi/10.1006/cimm.1995.1172}}
}

@article{Guleria200716,
author = {Guleria, Indira and Bupp, Melanie Gubbels and Dada, Shirine and Fife, Brian and Tang, Qizhi and Ansari, Mohammed Javeed and Trikudanathan, Subbulaxmi and Vadivel, Nidyanandh and Fiorina, Paolo and Yagita, Hideo and Azuma, Miyuki and Atkinson, Mark and Bluestone, Jeffrey A and Sayegh, Mohamed H},
title = {{Mechanisms of PDL1-mediated regulation of autoimmune diabetes}},
journal = {Clinical Immunology},
year = {2007},
volume = {125},
number = {1},
pages = {16--25},
keywords = {Negative costimulatory pathways},
doi = {10.1016/j.clim.2007.05.013},
rating = {0},
date-added = {2012-11-29T11:46:12GMT},
date-modified = {2012-11-29T11:46:12GMT},
abstract = {The PD-1--PDL1 pathway plays a critical role in regulating autoimmune diabetes as blockade or deficiency of PD-1 or PDL1 results in accelerated disease in NOD mice. We explored the cellular mechanisms involved in the regulation of these autoimmune responses by investigations involving various gene-deficient mice on the NOD background. Administration of blocking anti-PDL1 antibody to CD4+ T cell-deficient, CD8+ T cell-deficient and B cell-deficient mice demonstrated that PDL1-mediated regulation of autoreactive CD4+ and CD8+ T cells is critical for diabetes development. This concept was confirmed by adoptive transfer studies utilizing lymphocytes from BDC2.5 and 4.1 (CD4+) TCR transgenic mice and 8.3 (CD8+) TCR transgenic mice; efforts showing increased proliferation of both CD4+ and CD8+ T cells following PDL1 blockade in vivo. Furthermore, we observed that anti-PDL1-mediated acceleration is dependent upon events occurring in the pancreatic lymph nodes during early disease stages, but becomes independent of the pancreatic lymph nodes during later disease stages. These data provide strong evidence that PDL1 regulates autoimmune diabetes by limiting the expansion of CD4+ and CD8+ autoreactive T cells, and define the timing and locale of PDL1-mediated regulation of type 1 diabetes.},
url = {http://www.sciencedirect.com/science/article/pii/S1521661607012211},
uri = {\url{papers2://publication/doi/10.1016/j.clim.2007.05.013}}
}

@article{Ling20051089,
author = {Ling, Xiaofeng and Cota-Gomez, Adela and Flores, Natalia C and Hernandez-Saavedra, Daniel and McCord, Joe M and Marecki, John C and Haskins, Kathryn and McDuffie, Marcia and Powers, Katherine and Kench, Jennifer and Oka, Masahiko and McMurtry, Ivan and Flores, Sonia C},
title = {{Alterations in redox homeostasis and prostaglandins impair endothelial-dependent vasodilation in euglycemic autoimmune nonobese diabetic mice}},
journal = {Free Radical Biology and Medicine},
year = {2005},
volume = {39},
number = {8},
pages = {1089--1098},
keywords = {Endothelial dysfunction},
doi = {10.1016/j.freeradbiomed.2005.05.027},
rating = {0},
date-added = {2012-11-29T11:46:12GMT},
date-modified = {2012-11-29T11:46:12GMT},
abstract = {We report herein the novel observation that alterations in oxidant/antioxidant balance are evident and cause vascular dysfunction in aortae of prediabetic nonobese-diabetic mice (NOD). We found that nitrotyrosine, a biochemical marker of oxidant stress, was higher in the NOD aortae when compared to age-matched non-autoimmune BALB/c controls or the diabetes-resistant NOD congenic strain, NOD.Lc7. The oxidant stress was localized to the intimal and medial layers, and endothelium-dependent relaxation to acetylcholine was decreased in isolated aortic rings from NOD mice. Inhibition of nitric oxide synthesis caused an endothelium-dependent contraction, and treatment with either a selective thromboxane A2/prostaglandin H2 receptor antagonist or a non-isozyme-specific cyclooxygenase inhibitor reversed this effect. Aortic rings from NOD.Lc7 did not display the paradoxical vasoconstriction. Furthermore, the vascular dysfunction was caused by oxidative stress, as treatment with a superoxide dismutase mimetic in vivo or with native antioxidant enzymes ex vivo inhibited the tissue oxidant stress and restored endothelium-dependent relaxation. Endothelial function was also restored by the inhibitors of NAD(P)H oxidase, diphenylene iodonium or apocynin. Our studies indicate that an oxidant stress that occurs prior to the onset of diabetes in this mouse model contributes to endothelial dysfunction independently of overt diabetes.},
url = {http://www.sciencedirect.com/science/article/pii/S0891584905003084},
uri = {\url{papers2://publication/doi/10.1016/j.freeradbiomed.2005.05.027}}
}

