affiliation = {Laboratory of Applied Immunobiology, University of Zurich, Gloriastrasse 30, CH-8006 Zurich, Switzerland. onur.boyman@uzh.ch},
doi = {10.1038/nri3156},
pmid = {22343569},
language = {English},
read = {Yes},
