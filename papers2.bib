@article{Brandao2010277,
author = {Brandao, Lucas C and Vatta, Serena and Guimaraes, Rafael and Segat, Ludovica and Araujo, Jaqueline and De Lima Filho, Jos{\`e} L and Arraes, Luiz C and Not, Tarcisio and Crovella, Sergio},
title = {{Rapid genetic screening for major human leukocyte antigen risk haplotypes in patients with type 1 diabetes from Northeastern Brazil}},
journal = {Human Immunology},
