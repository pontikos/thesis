@article{McKenna2011,
author = {McKenna, Mary C},
title = {{Glutamate dehydrogenase in brain mitochondria: do lipid modifications and transient metabolon formation influence enzyme activity?}},
journal = {Neurochemistry international},
year = {2011},
volume = {59},
number = {4},
pages = {525--533},
publisher = {Elsevier Ltd},
