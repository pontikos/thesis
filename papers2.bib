@article{Funda1995179,
author = {Funda, David and Stenvang, Jens Peter and Buschard, Karsten},
title = {{Age-related changes in T$\gamma$$\delta$ cells of NOD mice}},
journal = {Immunology Letters},
year = {1995},
volume = {45},
number = {3},
pages = {179--184},
keywords = {$\gamma$$\delta$T cell},
doi = {10.1016/0165-2478(95)00003-N},
rating = {0},
date-added = {2012-11-29T11:45:55GMT},
date-modified = {2012-11-29T11:45:55GMT},
abstract = {T$\gamma$$\delta$ cells have been reported to recognize both mycobacterial and human heat-shock proteins (HSP), and a possible role of 65 kDa HSP has been suggested also in the pathogenesis of insulin-dependent diabetes mellitus. The aim of this study was to investigate age-related changes of T$\gamma$$\delta$ cells during diabetes development in non-obese diabetic (NOD) mice. Using FACS analysis relative numbers of T$\gamma$$\delta$+ cells from thymus, blood and spleen were determined in 3-week-old non-diabetic, at onset of diabetes, and 1-week diabetic NOD mice and corresponding BALB/cJ controls. In comparison to BALB/cJ mice, higher values (2.4 $\pm$ 0.2% vs. 1.1 $\pm$ 0.1%) were found in the thymus of 3-week-old NOD mice (P {\&}lt; 0.01) as well as in spleens of 22-week-old littermates (1.1 $\pm$ 0.1% vs. 0.6 $\pm$ 0.1%, P {\&}lt; 0.01). In addition, a higher proportion of T$\gamma$$\delta$ cells was observed in blood samples of all age groups of NOD as compared to BALB/cJ mice, with values 3.5 $\pm$ 0.7% (P {\&}lt; 0.05) in 3-week-old to 4.4 $\pm$ 0.9% and 3.7 $\pm$ 0.3% (P {\&}lt; 0.01) in 16- and 22-week-old NOD littermates. Differences in TCR$\gamma$$\delta$ expression did not influence the whole CD3+ subset of mononuclear cells. Thus, our results show relatively higher numbers of T$\gamma$$\delta$ cells in NOD mice and their increase in the periphery at onset of diabetes and later may suggest that T$\gamma$$\delta$ cells participate in $\beta$-cell destruction.},
url = {http://www.sciencedirect.com/science/article/pii/016524789500003N},
uri = {\url{papers2://publication/doi/10.1016/0165-2478(95)00003-N}}
}

@article{Wen2008282,
author = {Wen, Yanting and Ouyang, Jian and Yang, Rong and Chen, Junhao and Liu, Yong and Zhou, Xiaojun and Burt, Richard K},
