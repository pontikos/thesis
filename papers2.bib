uri = {\url{papers2://publication/doi/10.1016/j.immuni.2009.04.007}}
}

@article{PerezBravo200637,
author = {Perez-Bravo, Francisco and Martinez-Laso, Jorge and Martin-Villa, Jose M and Moscoso, Juan and Moreno, Almudena and Serrano-Vela, Juan I and Zamora, Jorge and Asenjo, Silvia and Gleisner, Andrea and Arnaiz-Villena, Antonio},
