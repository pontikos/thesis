journal = {Journal of immunological methods},
year = {2010},
volume = {361},
number = {1-2},
pages = {1--20},
month = sep,
affiliation = {Department of Chemistry, University of Toronto, 80 St. George St., Toronto ON, Canada M5S 3H6; DVS Sviences Inc., 70 Peninsula Cr., Richmond Hill, ON L4S1Z5, Canada. olga.ornatsky@utoronto.ca},
doi = {10.1016/j.jim.2010.07.002},
