@article{Wagener:1982up,
author = {Wagener, D K and Sacks, J M and LaPorte, R E and Macgregor, J M},
title = {{The Pittsburgh study of insulin-dependent diabetes mellitus. Risk for diabetes among relatives of IDDM.}},
journal = {Diabetes},
year = {1982},
