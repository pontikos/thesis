abstract = {This article describes the multiple escape mechanisms used by tumor cells to avoid T-cell--mediated recognition and destruction. The discussion focuses on escape mechanisms that may result from changes at the level of TA-specific cytotoxic T lymphocytes and tumor cells in the tumor microenvironment. Specifically, we discuss the negative impact of regulatory T cells and T-cell apoptosis on the tumor antigen--specific cytotoxic T lymphocyte response. We also discuss changes in the expression of histocompatibility antigens by tumor cells, which may affect tumor cell--immune cell interactions.},
url = {http://www.sciencedirect.com/science/article/pii/S1055320707000981},
uri = {\url{papers2://publication/doi/10.1016/j.soc.2007.08.004}}
}

@article{Kay1991263,
author = {Kay, Thomas W H and Campbell, Iain L and Harrison, Leonard C},
title = {{Characterization of pancreatic T lymphocytes associated with beta cell destruction in the non-obese diabetic (NOD) mouse}},
journal = {Journal of Autoimmunity},
year = {1991},
volume = {4},
number = {2},
pages = {263--276},
doi = {10.1016/0896-8411(91)90023-6},
rating = {0},
date-added = {2012-11-29T11:45:33GMT},
date-modified = {2012-11-29T11:45:33GMT},
abstract = {Pancreatic beta cell destruction in the non-obese diabetic (NOD) mouse is mediated by T lymphocytes and macrophages and accelerated by cyclophosphamide. We purified pancreatic T lymphocytes from the NOD mouse for comparative phenotypic and functional analysis with T lymphocytes from spleen, peripheral blood and regional lymph nodes. Pancreatic T lymphocytes from NOD-Wehi mice, which have an incidence of spontaneous diabetes of {\&}lt;5%, had a CD4:CD8 ratio of 1.25 $\pm$ 0.23 compared with 2.44 $\pm$ 0.31 for peripheral blood lymphocytes. After cyclophosphamide, the CD4:CD8 ratio of pancreatic lymphocytes increased to 2.30 $\pm$ 0.24 at day 7. T lymphocytes bearing IL-2 receptors increased two- to three-fold in number and their secretion of GM-CSF/IL-3 and IFN-$\gamma$ increased to a maximum on day 7. Pancreatic insulin content and mRNA levels declined sharply between days 10 and 12, at which time the majority of pancreatic T lymphocytes in hyperglycaemic mice were CD8+ (CD4:CD8 ratio 0.63 $\pm$ 0.04 compared to 4.14 $\pm$ 1.05 in peripheral blood). The pancreatic T lymphocyte CD4:CD8 ratio in prediabetic NOD-Lt mice, which have an incidence of spontaneous diabetes of about 60% at 150 days, was similar to that in untreated NOD-Wehi mice, but 25% of their pancreatic CD8 T lymphocytes were IL-2-receptor positive. Thus, significant changes in the phenotype of NOD pancreatic T lymphocytes following cyclophosphamide were not reflected in peripheral blood or spleen T lymphocytes. The earliest change after cyclophosphamide was an increase in activated, predominantly CD4+ T lymphocytes; with the development of beta cell destruction and hyperglycaemia, pancreatic T lymphocytes were, as in human IDDM, predominantly CD8+.},
url = {http://www.sciencedirect.com/science/article/pii/0896841191900236},
uri = {\url{papers2://publication/doi/10.1016/0896-8411(91)90023-6}}
}

@article{Waldmann2009613,
author = {Waldmann, Herman and Cobbold, Stephen},
