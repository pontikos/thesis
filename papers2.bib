abstract = {Type 1 diabetes is a chronic autoimmune disease characterised by the selective destruction of pancreatic beta ($\beta$) cells. The understanding of the aetiology of this disease has increased dramatically in recent years by the study of tissue recovered from patients, from analysis of the responses of isolated islet and $\beta$-cells in tissue culture and via the use of animal models. However, knowledge of the immunopathology of type 1 diabetes in humans is still relatively deficient due largely to the difficulty of accessing appropriate samples. Here we review the state of current knowledge in relation to the histopathological features of the disease in humans. We focus specifically on recent-onset type 1 diabetes cases since in such patients, evidence of the ongoing disease process is still present. We chart the progression of the disease by describing the characteristic features of the pancreas, consider the sequence of immune cell infiltration and discuss the abnormalities of MHC antigen expression. The possibility that these changes might derive from a persistent enteroviral infection of the islet beta cells is examined.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20424842&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2011/Richardson/Semin%20Immunopathol%202011%20Richardson.pdf},
file = {{Semin Immunopathol 2011 Richardson.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2011/Richardson/Semin Immunopathol 2011 Richardson.pdf:application/pdf}},
uri = {\url{papers2://publication/livfe/id/37207}}
}

@article{Qin1998591,
author = {Qin, H Y and Elliott, J F and Lakey, J R T and Rajotte, R V and Singh, B},
title = {{Endogenous Immune Response to Glutamic Acid Decarboxylase (GAD67) in NOD Mice is Modulated by Adjuvant Immunotherapy}},
journal = {Journal of Autoimmunity},
year = {1998},
volume = {11},
number = {6},
pages = {591--601},
keywords = {Th2},
doi = {10.1006/jaut.1998.0243},
rating = {0},
date-added = {2012-11-29T11:45:47GMT},
date-modified = {2012-11-29T11:45:47GMT},
abstract = {We have shown that immunization of non-obese diabetic (NOD) mice with adjuvants (CFA or BCG) prevents the onset of diabetes by induction of regulatory cells. Since autoimmune responses to glutamic acid decarboxylase (GAD) are up-regulated in insulin-dependent diabetes mellitus (IDDM), in this study GAD67-specific antibody, T cell proliferation and lymphokine production patterns were analysed in the adjuvant-treated mice to characterize the regulatory mechanisms underlying the protection. We used both spontaneous diabetes and syngeneic islet transplantation models in NOD mice. Protection against spontaneous diabetes and prevention of syngeneic islet graft rejection by CFA or BCG treatment was found to be accompanied by the production of long lasting and high titre anti-GAD67 antibody of IgG1 isotype in the sera. Uponin vitrostimulation with GAD67, draining lymph node and spleen cells from CFA-immunized NOD mice or syngeneic islet-grafted and BCG-protected NOD mice produced much more IL-4, whereas there was no significant change in IFN-$\gamma$ production. The strong early T cell proliferative response to GAD67 in CFA or BCG-immunized NOD mice was followed by a low or unresponsiveness state. Taken together, these results suggest a shift in Th1/Th2 balance in the GAD67-specific endogenous immune response to a change in Th2 levels after adjuvant treatment. We postulate that the protective effect of CFA or BCG is due to the diversion of GAD-specific endogenous cellular immune response to a non-pathogenic humoral response.},
url = {http://www.sciencedirect.com/science/article/pii/S0896841198902438},
uri = {\url{papers2://publication/doi/10.1006/jaut.1998.0243}}
}

@article{Nakazawa2001119,
author = {Nakazawa, Tetsuya and Satoh, Jo and Takahashi, Kazuma and Sakata, Yoshiyuki and Ikehata, Fumiko and Takizawa, Yumiko and Bando, Shin-Ichiro and Housai, Toshimune and Li, Yan and Chen, Chen and Masuda, Takayuki and Kure, Shigeo and Kato, Ichiro and Takasawa, Shin and Taniguchi, Tadatsugu and Okamoto, Hiroshi and Toyota, Takayoshi},
title = {{Complete Suppression of Insulitis and Diabetes in NOD mice Lacking Interferon Regulatory Factor-1}},
journal = {Journal of Autoimmunity},
year = {2001},
volume = {17},
number = {2},
pages = {119--125},
keywords = {IRF-1, NOD mice, type 1 diabetes mellitus, Th1 and Th2 cytokine},
doi = {10.1006/jaut.2001.0531},
rating = {0},
date-added = {2012-11-29T11:45:48GMT},
date-modified = {2012-11-29T11:45:48GMT},
abstract = {Interferon regulatory factor-1 (IRF-1), a transcriptional factor, regulates type I interferon and interferon-induced genes. It was reported that IRF-1 regulates important molecules required for inflammation and immune reactions. To investigate the role of IRF-1 in the development of autoimmune diabetes, we established IRF-1 deficient (IRF-1−/−) non-obese diabetic (NOD) mice. IRF-1-deficient C57BL/6J mice were out-crossed to NOD mice, and F1 were backcrossed to NOD mice. At the N8 generation, the heterozygote for IRF-1 mutation was intercrossed and N8F1 was obtained. Out of three NOD genotypes, IRF-1+/+and IRF-1+/−developed spontaneous diabetes with an incidence of 47% (9/19) and 50% (10/20) by 30 weeks of age, respectively; whereas IRF-1−/−did not develop diabetes (0/18, P{\&}lt; 0.01 vs.+/+and+/−). Histologically, IRF-1+/+and IRF-1+/−had various degrees of insulitis, but IRF-1−/−had no insulitis. In comparison with IRF-1+/+, the percentage of CD4+and Mac-1+splenic cells significantly increased, whereas CD3+, CD8+and B220+cells decreased in IRF-1−/−. Furthermore, spleen cell proliferation in response to Con A or murine GAD65 peptide, a major autoantigen of the pancreatic $\beta$-cell, significantly increased, and the IFN-$\gamma$/IL-10 ratio in the culture supernatant significantly decreased in IRF-1−/−, suggesting Th2 deviation in cytokine balance. These results indicate that IRF-1 plays a key role in developing insulitis and diabetes in NOD mice.},
url = {http://www.sciencedirect.com/science/article/pii/S0896841101905311},
uri = {\url{papers2://publication/doi/10.1006/jaut.2001.0531}}
}

