@article{Culverhouse:2002ep,
author = {Culverhouse, Robert and Suarez, Brian K and Lin, Jennifer and Reich, Theodore},
title = {{A perspective on epistasis: limits of models displaying no main effect.}},
journal = {American Journal of Human Genetics},
year = {2002},
volume = {70},
number = {2},
pages = {461--471},
month = feb,
affiliation = {Department of Psychiatry, Washington University School of Medicine, St. Louis, MO 63110-1093, USA. rob@frodo.wustl.edu},
doi = {10.1086/338759},
pmid = {11791213},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-02-19T13:02:59GMT},
date-modified = {2013-09-30T11:16:38GMT},
abstract = {The completion of a draft sequence of the human genome and the promise of rapid single-nucleotide-polymorphism-genotyping technologies have resulted in a call for the abandonment of linkage studies in favor of genome scans for association. However, there exists a large class of genetic models for which this approach will fail: purely epistatic models with no additive or dominance variation at any of the susceptibility loci. As a result, traditional association methods (such as case/control, measured genotype, and transmission/disequilibrium test [TDT]) will have no power if the loci are examined individually. In this article, we examine this class of models, delimiting the range of genetic determination and recurrence risks for two-, three-, and four-locus purely epistatic models. Our study reveals that these models, although giving rise to no additive or dominance variation, do give rise to increased allele sharing between affected sibs. Thus, a genome scan for linkage could detect genomic subregions harboring susceptibility loci. We also discuss some simple multilocus extensions of single-locus analysis methods, including a conditional form of the TDT.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=11791213&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2002/Culverhouse/Am%20J%20Hum%20Genet%202002%20Culverhouse.pdf},
file = {{Am J Hum Genet 2002 Culverhouse.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2002/Culverhouse/Am J Hum Genet 2002 Culverhouse.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/338759}}
}

