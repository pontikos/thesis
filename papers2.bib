@article{Marrero20129,
author = {Marrero, Idania and Vong, Allen and Dai, Yang and Davies, Joanna D},
title = {{T cell populations in the pancreatic lymph node naturally and consistently expand and contract in NOD mice as disease progresses}},
journal = {Molecular Immunology},
year = {2012},
volume = {52},
number = {1},
pages = {9--18},
keywords = {Perturbation},
doi = {10.1016/j.molimm.2012.04.004},
rating = {0},
date-added = {2012-11-29T11:46:23GMT},
date-modified = {2012-11-29T11:46:23GMT},
abstract = {Nonobese diabetic (NOD) mice develop spontaneous autoimmune Type 1 diabetes (T1D) that results from the destruction of insulin secreting $\beta$ cells by diabetogenic T cells. The activation of autoreactive T cells occurs in the pancreatic lymph nodes (PLN) from where effector T cells migrate to the pancreas. This study was designed to explore whether T cell populations in the NOD PLN expand in a predictable and reproducible way during disease progression. Complementary determining region (CDR) 3 length spectratype analysis of 19 TCR V$\beta$ families was used to identify the relative frequency of T populations in PLN of 4 and 10 week old NOD mice and mice at T1D onset. Significant and highly reproducible changes in specific T cell populations were detected in 14 of V$\beta$ families tested at all stages of disease. However, of these, the CDR3 spectratype of only four V$\beta$ families was significantly more perturbed at T1D onset than in 10 week old mice. Intriguingly, when diabetes was induced in 10 week old mice with cyclophosphamide (CYP) the same four V$\beta$ families, V$\beta$5.1, V$\beta$9, V$\beta$10, and V$\beta$15, were again significantly more perturbed than in the untreated non-diabetic age matched mice. Taken together the data show that while T cell responses in PLN of NOD mice are heterogeneous, they are ordered and consistent throughout disease development. The finding that within this heterogeneous response four V$\beta$ families are significantly more perturbed in diabetic mice, whether spontaneous or induced, strongly suggests their selection as part of the disease process.},
url = {http://www.sciencedirect.com/science/article/pii/S0161589012002465},
uri = {\url{papers2://publication/doi/10.1016/j.molimm.2012.04.004}}
}

