@article{Stadinski2010446,
author = {Stadinski, Brian and Kappler, John and Eisenbarth, George S},
title = {{Molecular Targeting of Islet Autoantigens}},
journal = {Immunity},
year = {2010},
volume = {32},
number = {4},
pages = {446--456},
doi = {10.1016/j.immuni.2010.04.008},
rating = {0},
date-added = {2012-11-29T11:46:03GMT},
date-modified = {2012-11-29T11:46:03GMT},
abstract = {Type 1 diabetes of man and animal models results from immune-mediated specific beta cell destruction. Multiple islet antigens are targets of autoimmunity and most of these are not beta cell specific. Immune responses to insulin appear to be essential for the development of diabetes of the NOD mouse. In this review, we will emphasize the unusual manner in which selected autoantigenic peptides (particularly the recently discovered target of BDC2.5 T cells [chromagranin A]) are presented and recognized by autoreactive CD4+ T cell receptors. We hypothesize that ``unusual'' structural interactions of specific trimolecular complexes (MHC class II, peptide, and T cell receptors) are fundamental to the escape from the thymus of autoreactive T cells able to cause type 1 diabetes.},
url = {http://www.sciencedirect.com/science/article/pii/S1074761310001330},
uri = {\url{papers2://publication/doi/10.1016/j.immuni.2010.04.008}}
}

