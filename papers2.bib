@article{Schwikowski:2000bu,
author = {Schwikowski, B and Uetz, P and Fields, S},
title = {{A network of protein-protein interactions in yeast}},
journal = {Nature Biotechnology},
year = {2000},
volume = {18},
number = {12},
pages = {1257--1261},
affiliation = {Inst Syst Biol, Seattle, WA 98105 USA},
language = {English},
read = {Yes},
