local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2007/Lowe/Nat.%20Genet.%202007%20Lowe.pdf},
file = {{Nat. Genet. 2007 Lowe.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2007/Lowe/Nat. Genet. 2007 Lowe.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/ng2102}}
}

