@article{Shapiro:2008dq,
author = {Shapiro, Howard M and Perlmutter, Nancy G},
title = {{Killer applications: toward affordable rapid cell-based diagnostics for malaria and tuberculosis.}},
journal = {Cytometry Part B: Clinical Cytometry},
year = {2008},
volume = {74 Suppl 1},
pages = {S152--64},
affiliation = {The Center for Microbial Cytometry, West Newton, Massachusetts 02465-2513, USA. hms@shapirolab.com},
doi = {10.1002/cyto.b.20401},
pmid = {18228564},
language = {English},
