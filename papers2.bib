@article{Sironi2010421,
author = {Sironi, Manuela and Clerici, Mario},
title = {{The hygiene hypothesis: an evolutionary perspective}},
journal = {Microbes and Infection},
year = {2010},
volume = {12},
number = {6},
pages = {421--427},
keywords = {Host--pathogen interactions},
doi = {10.1016/j.micinf.2010.02.002},
