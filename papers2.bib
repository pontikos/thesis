abstract = {A novel immunosuppressive compound, FK-506, isolated from Streptomyces has potent immunosuppressive activities. To investigate the effect of FK-506 on the course of diabetes in nonobese diabetic (NOD) mice, we gave this drug to these animals, from the age of 8 weeks, intraperitoneally, in doses of 0.1 mg (2.5 mg/kg/day) or 0.01 mg (0.25 mg/kg/day) three times a week. Overt diabetes were observed in 75.5% of control mice by the age of 20 weeks. In contrast, no diabetes occurred in mice given 0.1 mg of FK-506. Sixteen percent of mice treated with 0.01 mg of the drug became diabetic. Administration of this drug prevented the progression of insulitis in NOD mice. The mice given 0.1 mg of FK-506 lost weight, but this was reversible.},
url = {http://www.sciencedirect.com/science/article/pii/009012299090041N},
uri = {\url{papers2://publication/doi/10.1016/0090-1229(90)90041-N}}
}

@article{Magombedze201088,
author = {Magombedze, Gesham and Nduru, Polite and Bhunu, Claver P and Mushayabasa, Steady},
title = {{Mathematical modelling of immune regulation of type 1 diabetes}},
journal = {Biosystems},
year = {2010},
volume = {102},
number = {2--3},
pages = {88--98},
keywords = {Autolytic T cells},
doi = {10.1016/j.biosystems.2010.07.018},
rating = {0},
date-added = {2012-11-29T11:46:22GMT},
date-modified = {2012-11-29T11:46:22GMT},
abstract = {Type 1 diabetes is a disease characterized by progressive loss of $\beta$ cell function due to an autoimmune reaction affecting the islets of Langerhans. Two types of T cells are involved in diabetes: turncoat auto-reactive T cells, or T cells gone bad, that kill the insulin-producing cells, and regulatory T cells that are unable to control the auto-reactive T cells. We formulate a mathematical model that incorporates the role of cytotoxic T cells and regulatory T cells in type 1 diabetes. This study shows that onset of type 1 diabetes is due to a collective, dynamical instability, rather than being caused by a single etiological factor. It is also a numbers game between regulatory T cells and auto-reactive T cells. The problem in the onset of this disease is that there are not enough of the regulatory cells that suppress the immune response against the body's insulin-producing pancreatic islet cells.},
url = {http://www.sciencedirect.com/science/article/pii/S0303264710001267},
uri = {\url{papers2://publication/doi/10.1016/j.biosystems.2010.07.018}}
}

