@article{Scharpf:2008iya,
author = {Scharpf, Robert B and Parmigiani, Giovanni and Pevsner, Jonathan and Ruczinski, Ingo},
title = {{Hidden Markov models for the assessment of chromosomal alterations using high-throughput SNP arrays.}},
journal = {The Annals of Applied Statistics},
year = {2008},
volume = {2},
number = {2},
pages = {687--713},
month = jun,
affiliation = {Department of Biostatistics, Johns Hopkins Bloomberg School of Public Health, Baltimore, MD 21205.},
doi = {10.1214/07-AOAS155},
pmid = {19609370},
