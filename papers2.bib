uri = {\url{papers2://publication/doi/10.1016/j.cellimm.2007.07.001}}
}

@article{Black2006236,
author = {Black, Sasha P and Constantinidis, Ioannis and Cui, Hong and Tucker-Burden, Carol and Weber, Collin J and Safley, Susan A},
