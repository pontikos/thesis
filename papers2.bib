author = {Krzanowski, Wojtek J and Bailey, Trevor C and Partridge, Derek and Fieldsend, Jonathan E and Everson, Richard M and Schetinin, Vitaly},
title = {{Confidence in classification: A Bayesian approach}},
journal = {Journal of Classification},
year = {2006},
volume = {23},
number = {2},
pages = {199--220},
affiliation = {Univ Exeter, Sch Engn Comp Sci {\&} Math, Exeter EX4 4QE, Devon, England},
