@article{Payne2005435,
author = {Payne, Felicity and Smyth, Deborah J and Pask, Rebecca and Cooper, Jason D and Masters, Jennifer and Wang, William Y S and Godfrey, Lisa M and Bowden, Georgina and Szeszko, Jeffrey and Smink, Luc J and Lam, Alex C and Burren, Oliver and Walker, Neil M and Nutland, Sarah and Rance, Helen and Undlien, Dag E and R{\o}nningen, Kjersti S and Guja, Cristian and Ionescu-T{\^\i}rgovi{\c s}te, Constantin and Todd, John A and Twells, Rebecca C J},
