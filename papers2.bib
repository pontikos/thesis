keywords = {Bacillus subtilis,Bacillus subtilis: metabolism,Bacterial,Bacterial Proteins,Bacterial Proteins: metabolism,Gene Expression Regulation,Glucose,Glucose: deficiency,Glucose: metabolism,Mass Spectrometry,Proteome,Proteome: analysis,Proteome: metabolism,Proteomics,Proteomics: methods},
