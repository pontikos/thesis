author = {Lepr{\^e}tre, F and Villenet, C and Quief, S and Nibourel, O and Jacquemin, C and Troussard, X and Jardin, F and Gibson, F and Kerckaert, J P and Roumier, C and Figeac, M},
title = {{Waved aCGH: to smooth or not to smooth.}},
