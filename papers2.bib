file = {{Tissue Antigens 2007 Thomson.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2007/Thomson/Tissue Antigens 2007 Thomson.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1399-0039.2007.00867.x}}
}

