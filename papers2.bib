@article{Ortmann2005689,
author = {Ortmann, Jana and Nett, Philipp C and Celeiro, Jennifer and Traupe, Tobias and Tornillo, Luigi and Hofmann-Lehmann, Regina and Haas, Elvira and Frank, Beat and Terraciano, Luigi M and Barton, Matthias},
title = {{Endothelin inhibition delays onset of hyperglycemia and associated vascular injury in type I diabetes: Evidence for endothelin release by pancreatic islet $\beta$-cells}},
