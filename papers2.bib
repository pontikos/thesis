date-modified = {2012-11-29T11:46:29GMT},
abstract = {Genome-wide association scans in type 2 diabetes (T2D) have identified a risk variant, rs13266634 (Arg325Trp), in SLC30A8 on chromosome 8. SLC30A8 encodes a $\beta$-cell specific zinc-ion transporter and rs13266634 has been shown to affect insulin secretion. Recently, autoantibodies for Slc30A8 with high predictive value were demonstrated in individuals with type 1 diabetes (T1D), making this gene an interesting T1D candidate gene. We genotyped rs13266634 in 3008 cases and controls and 246 families from Denmark. Association to T1D could not be demonstrated.},
url = {http://www.sciencedirect.com/science/article/pii/S1096719208000589},
uri = {\url{papers2://publication/doi/10.1016/j.ymgme.2008.02.011}}
}

@article{Santamaria2001663,
author = {Santamaria, Pere},
title = {{Effector lymphocytes in autoimmunity}},
journal = {Current Opinion in Immunology},
year = {2001},
volume = {13},
number = {6},
pages = {663--669},
keywords = {autoantigens},
doi = {10.1016/S0952-7915(01)00276-X},
rating = {0},
date-added = {2012-11-29T11:46:29GMT},
date-modified = {2012-11-29T11:46:29GMT},
abstract = {Autoimmune diseases result from complex interactions among different T- and B-lymphocyte subpopulations that target a rapidly growing number of autoantigens on different cell types. The etiology of most spontaneous autoimmune disorders, and both the kinetics and hierarchy of the underlying autoimmune responses are poorly understood. However, important advances have been made in recent years in our understanding of how autoreactive lymphocytes cause tissue damage, including the discovery that granzyme B binds to a cell surface receptor on target cells. This review is an attempt to summarize recent developments in this area.},
url = {http://www.sciencedirect.com/science/article/pii/S095279150100276X},
