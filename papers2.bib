publisher = {Academic Press},
doi = {10.1016/S0065-2776(08)00801-8},
rating = {0},
date-added = {2012-11-29T11:46:00GMT},
date-modified = {2012-11-29T11:46:00GMT},
abstract = {Publisher Summary This chapter discusses several key areas of research on the pathogenesis of Type 1 diabetes mellitus (T1DM) in both man and the principal animal model, the non-obese diabetic (NOD) mouse. Many of these have subsequently been found in patients with both prediabetes and recent-onset diabetes. New therapeutic approaches have been developed in the NOD model and some of these have been tested in patients with recent-onset T1DM, which is complex and chronic autoimmune disease with a serious clinical impact. It is estimated that approximately 1--1.5 million patients have T1DM in the United States. During that time there has been a rough doubling of the incidence of diabetes in relatively stable population. 15 distinct B cell derived peptides and the corresponding CD4 T cells have been identified among various laboratories. The searches have followed different approaches: testing T cell lines or clones on proteins known to be part of the B cell secretory granules and (or) proteins first identified by autoantibodies. Effects of administration of glutamic acid decarboxylase 65 (GAD 65) to patients with recent-onset T1D, and the prospect for trials to prevent T1DM in susceptible human populations are also described in the chapter.},
url = {http://www.sciencedirect.com/science/article/pii/S0065277608008018},
uri = {\url{papers2://publication/doi/10.1016/S0065-2776(08)00801-8}}
}

@article{Lenschow1996285,
author = {Lenschow, Deborah J and Herold, Kevan C and Rhee, Lesley and Patel, Bina and Koons, Ann and Qin, Hui-Yu and Fuchs, Elaine and Singh, Bhagarith and Thompson, Craig B and Bluestone, Jeffrey A},
