@article{Weaver:2010ie,
author = {Weaver, Suzanne and Dube, Simant and Mir, Alain and Qin, Jian and Sun, Gang and Ramakrishnan, Ramesh and Jones, Robert C and Livak, Kenneth J},
title = {{Taking qPCR to a higher level: Analysis of CNV reveals the power of high throughput qPCR to enhance quantitative resolution.}},
journal = {Methods (San Diego, Calif.)},
year = {2010},
volume = {50},
number = {4},
pages = {271--276},
month = apr,
affiliation = {Fluidigm Corporation, 7000 Shoreline Court, Suite 100, South San Francisco, CA 94080, USA.},
doi = {10.1016/j.ymeth.2010.01.003},
pmid = {20079846},
language = {English},
rating = {0},
date-added = {2011-10-25T09:17:01GMT},
date-modified = {2013-10-15T17:42:01GMT},
abstract = {This paper assesses the quantitative resolution of qPCR using copy number variation (CNV) as a paradigm. An error model is developed for real-time qPCR data showing how the precision of CNV determination varies with the number of replicates. Using samples with varying numbers of X chromosomes, experimental data demonstrates that real-time qPCR can readily distinguish four copes from five copies, which corresponds to a 1.25-fold difference in relative quantity. Digital PCR is considered as an alternative form of qPCR. For digital PCR, an error model is shown that relates the precision of CNV determination to the number of reaction chambers. The quantitative capability of digital PCR is illustrated with an experiment distinguishing four and five copies of the human gene MRGPRX1. For either real-time qPCR or digital PCR, practical application of these models to achieve enhanced quantitative resolution requires use of a high throughput PCR platform that can simultaneously perform thousands of reactions. Comparing the two methods, real-time qPCR has the advantage of throughput and digital PCR has the advantage of simplicity in terms of the assumptions made for data analysis.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20079846&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2010/Weaver/Methods%202010%20Weaver.pdf},
file = {{Methods 2010 Weaver.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2010/Weaver/Methods 2010 Weaver.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.ymeth.2010.01.003}}
}

