@article{Slavin1996627,
author = {Slavin, Shimon and Weiss, Lola and Wenlang, Xia and Gross, David J},
title = {{Successful treatment of diabetes in nod mice with advanced disease by islet isografts following immunoregulation with Linomide (quinoline-3-carboxamide)}},
journal = {Cell Transplantation},
year = {1996},
volume = {5},
