@article{Zamir:2005ex,
author = {Zamir, Eli and Geiger, Benjamin and Cohen, Nir and Kam, Zvi and Katz, Ben-Zion},
title = {{Resolving and classifying haematopoietic bone-marrow cell populations by multi-dimensional analysis of flow-cytometry data.}},
journal = {British journal of haematology},
year = {2005},
volume = {129},
number = {3},
pages = {420--431},
month = may,
affiliation = {The Haematology Institute, Tel-Aviv Sourasky Medical Centre, Tel-Aviv 64239, Israel.},
doi = {10.1111/j.1365-2141.2005.05471.x},
pmid = {15842667},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-09-07T13:46:34GMT},
date-modified = {2012-12-10T14:25:17GMT},
abstract = {The study of normal or malignant haematopoiesis requires the analysis of heterogeneous cell populations using multiple morphological and molecular criteria. Flow cytometry has the capacity to acquire multi-parameter information of large haematopoietic cell populations, utilizing various combinations of >200 molecular markers (clusters of differentiation, CD). However, current flow cytometry analyses are based on serial gating of two-parametric scatter plots--a process that is inherently incapable to discriminate all subgroups of cells in the data. Here we studied the cellular diversity of normal bone marrows (BM) using multi-dimensional cluster analysis of six-parametric flow cytometry data (four CD, forward scatter and side scatter), focusing mainly on the myeloid lineage. Twenty-three subclasses of cells were resolved, many of them inseparable even when examined in all possible two-parametric scatter plots. The multi-dimensional analysis could distinguish the haematopoietic progenitors according to International Society of Haematotherapy and Graft Engineering criteria from other types of immature cells. Based on the defined clusters, we designed a classifier that assigns BM cells in samples to subclasses based on robust six-dimensional position and extended shape. The analysis presented here can manage successfully both the increasing numbers of haematopoietic cellular markers and sample heterogeneity. This should enhance the ability to study normal haematopoiesis, and to identify and monitor haematopoietic disorders.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=15842667&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2005/Zamir/Br.%20J.%20Haematol.%202005%20Zamir.pdf},
file = {{Br. J. Haematol. 2005 Zamir.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2005/Zamir/Br. J. Haematol. 2005 Zamir.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1365-2141.2005.05471.x}}
}

@article{Graewin2004824,
author = {Graewin, Shannon J and Kiely, James M and Lee, Keun-Ho and Svatek, Carol L and Nakeeb, Attila and Pitt, Henry A},
title = {{Nonobese diabetic mice have diminished gallbladder motility and shortened crystal observation time}},
journal = {Journal of Gastrointestinal Surgery},
year = {2004},
volume = {8},
number = {7},
pages = {824--830},
keywords = {motility},
doi = {10.1016/j.gassur.2004.06.014},
rating = {0},
date-added = {2012-11-29T11:45:32GMT},
date-modified = {2012-11-29T11:45:32GMT},
abstract = {Diabetes and obesity are strongly associated and are risk factors for cholesterol gallstone disease. Leptin-deficient and leptin-resistant diabetic obese mice have enlarged, hypomotile gallbladders. In addition, bile from gallbladders of leptin-deficient mice has enhanced cholesterol crystal formation, whereas bile from gallbladders of leptin-resistant mice has delayed crystal observation time. To determine the effect of diabetes alone, we hypothesized that leptin-normal, nonobese diabetic (NOD) mice would have reduced biliary motility and rapid crystal formation. Twenty control and 9 prediabetic and 11 diabetic NOD, 12- to 26-week-old mice underwent glucose measurement and cholecystectomy for muscle bath stimulation with neurotransmitters. An additional group of 200 control and 78 NOD 12-week-old mice underwent microscopic bile examination for cholesterol crystal formation. Compared with control mice, prediabetic NOD mice had similar glucose levels and gallbladder volumes. Diabetic NOD mice had higher sugar levels and larger gallbladder volumes (P {\&}lt; 0.001) than control mice. Prediabetic NOD gallbladders had less contractility (P {\&}lt; 0.01) than control gallbladders, and contractility worsened (P {\&}lt; 0.01) in diabetic NOD mice. NOD mice formed cholesterol crystals earlier than did control mice (P {\&}lt; 0.05). Nonobese diabetic NOD mice have (1) decreased gallbladder contraction to neurotransmitters, which worsens with development of diabetes, and (2) rapid crystal formation. We conclude that diabetes alone alters gallbladder motility and cholesterol crystal formation.},
url = {http://www.sciencedirect.com/science/article/pii/S1091255X04002549},
uri = {\url{papers2://publication/doi/10.1016/j.gassur.2004.06.014}}
}

@article{Box:1964ut,
author = {Box, G E P and Cox, D R},
title = {{An Analysis of Transformations}},
