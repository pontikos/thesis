@article{Peltonen2012267,
author = {Peltonen, Juha E and Koponen, Anne S and Pullinen, Katri and H{\"a}gglund, Harriet and Aho, Jyrki M and Kyr{\"o}l{\"a}inen, Heikki and Tikkanen, Heikki O},
