title = {{Recombinant structures expand and contract inter and intragenic diversification at the KIR locus.}},
journal = {BMC Genomics},
year = {2013},
volume = {14},
pages = {89},
affiliation = {Clinical Research Division, Fred Hutchinson Cancer Research Center, Seattle, WA, USA.},
keywords = {KIR haplotype, KIR sequence},
doi = {10.1186/1471-2164-14-89},
pmid = {23394822},
language = {English},
read = {Yes},
