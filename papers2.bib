file = {{J Classif 2006 Krzanowski.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2006/Krzanowski/J Classif 2006 Krzanowski.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1007/s00357-006-0013-3}}
}

