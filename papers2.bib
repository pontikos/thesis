@article{Wang:2010iw,
author = {Wang, Peggy I. and Marcotte, Edward M},
title = {{It's the machine that matters: Predicting gene function and phenotype from protein networks}},
journal = {Journal of Proteomics},
year = {2010},
volume = {73},
number = {11},
pages = {2277--2289},
affiliation = {Univ Texas Austin, Ctr Syst {\&} Synthet Biol, Inst Cellular {\&} Mol Biol, Austin, TX 78712 USA},
doi = {10.1016/j.jprot.2010.07.005},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-02-09T11:07:10GMT},
date-modified = {2012-02-10T13:32:11GMT},
abstract = {Increasing knowledge about the organization of proteins into complexes, systems, and pathways has led to a flowering of theoretical approaches for exploiting this knowledge in order to better learn the functions of proteins and their roles underlying phenotypic traits and diseases. Much of this body of theory has been developed and tested in model organisms, relying on their relative simplicity and genetic and biochemical tractability to accelerate the research. In this review, we discuss several of the major approaches for computationally integrating proteomics and genomics observations into integrated protein networks, then applying guilt-by-association in these networks in order to identify genes underlying traits. Recent trends in this field include a rising appreciation of the modular network organization of proteins underlying traits or mutational phenotypes, and how to exploit such protein modularity using computational approaches related to the internet search algorithm PageRank. Many protein network-based predictions have recently been experimentally confirmed in yeast, worms, plants, and mice, and several successful approaches in model organisms have been directly translated to analyze human disease, with notable recent applications to glioma and breast cancer prognosis. (C) 2010 Elsevier B.V. All rights reserved.},
url = {http://links.isiglobalnet2.com/gateway/Gateway.cgi?GWVersion=2&SrcAuth=mekentosj&SrcApp=Papers&DestLinkType=FullRecord&DestApp=WOS&KeyUT=000283903000019},
local-url = {file://localhost/Users/nikolas/Library/Application%20Support/Papers2/Articles/2010/Wang/J%20Proteomics%202010%20Wang.pdf},
file = {{J Proteomics 2010 Wang.pdf:/Users/nikolas/Library/Application Support/Papers2/Articles/2010/Wang/J Proteomics 2010 Wang.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.jprot.2010.07.005}}
}

