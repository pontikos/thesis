uri = {\url{papers2://publication/doi/10.1038/35036627}}
}

@article{Otto2010,
author = {Otto, Andreas and Bernhardt, J\"{o}rg and Meyer, Hanna and Schaffer, Marc and Herbst, Florian-A and Siebourg, Juliane and M a der, Ulrike and Lalk, Michael and Hecker, Michael and Becher, D\"{o}rte},
